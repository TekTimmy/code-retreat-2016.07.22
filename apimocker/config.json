{
  "mockDirectory": "apimocker",
  "quiet": false,
  "port": "8001",
  "latency": 100,
  "allowedHeaders": ["Content-Type"],
  "webServices": {
    "response/empty/ok": {
      "mockFile": "responses/empty.json",
      "httpStatus": 200,
      "verbs": [
        "get",
        "post"
      ]
    },
    "response/empty/error": {
      "mockFile": "responses/empty.json",
      "httpStatus": 400,
      "verbs": [
        "get",
        "post"
      ]
    },
    "user/:id": {
      "switch": "id",
      "verbs": [
        "get"
      ],
      "mockFile": "responses/user/user.json"
    },
    "bitcoin": {
      "mockFile": "responses/bitcoin/values.json",
      "httpStatus": 200,
      "verbs": [
        "get"
      ]
    }
  }
}
